{
  "kind":"ReplicationController",
  "apiVersion":"v1",
  "metadata":{
    "name":"nginx",
    "labels":{
      "visualize":"true",
      "name":"nginx"
    }
  },
  "spec":{
    "replicas": 1,
    "selector":{
      "name":"nginx"
    },
    "template":{
      "metadata":{
        "labels":{
          "visualize":"true",
          "name":"nginx",
          "app": "sample",
          "version": "1.0",
          "uses": "nginx"
        }
      },
      "spec":{
        "containers":[
          {
            "name":"nginx",
            "image":"pankaj2934/rpi-nginx-test:1.0"
          }
        ]
      }
    }
  }
}
